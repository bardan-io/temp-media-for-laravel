{
  "version": "1.0.0",
  "name": "bardan-io/temp-media-for-laravel",
  "description": "Laravel package for handling temporary media uploads with automatic cleanup",
  "keywords": [
    "laravel",
    "package",
    "media",
    "upload",
    "temporary",
    "spatie",
    "media-library"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Achraf Bardan",
      "email": "achrafbardan@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2|^8.3",
    "spatie/laravel-medialibrary": "^10.0|^11.0|^12.0",
    "laravel/framework": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "BardanIO\\TempMedia\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BardanIO\\TempMedia\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "BardanIO\\TempMedia\\TempMediaServiceProvider"
      ],
      "aliases": {
        "TempMedia": "BardanIO\\TempMedia\\Facades\\TempMedia"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
